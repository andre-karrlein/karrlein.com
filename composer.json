{
    "name": "andre-karrlein/lumen-light",
    "description": "The Laravel Lumen Framework light version.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^5.6.4 || ^7.0",
        "laravel/lumen-framework": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}
